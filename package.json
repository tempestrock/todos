    "@remix-run/react": "2.12.0",
    "@remix-run/serve": "2.12.0",
    "amazon-cognito-identity-js": "6.3.12",
    "aws-amplify": "6.6.0",
    "aws-sdk": "2.1691.0",
    "isbot": "5.1.17",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1"
